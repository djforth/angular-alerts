{
  "name": "@djforth/angular-alerts",
  "version": "1.1.0",
  "description": "Angular Alerts",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "test:mac": "asset-pipeline-karma -b",
    "test:watch": "asset-pipeline-karma -w",
    "test": "./node_modules/.bin/asset-pipeline-karma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/angular-alerts.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "bugs": {
    "url": "https://github.com/djforth/angular-alerts/issues"
  },
  "keywords": [
    "Angular Alerts"
  ],
  "author": "Adrian Stainforth",
  "license": "MIT",
  "homepage": "https://github.com/djforth/angular-alerts",
  "assets": {
    "assets_in": "src",
    "assets_out": "lib",
    "javascripts": {},
    "karma": {
      "externals": [
        "./node_modules/babel-polyfill/dist/polyfill.js"
      ],
      "input": "./spec",
      "specs": [
        "/*_spec.es6.js"
      ]
    }
  },
  "devDependencies": {
    "@djforth/angular-jasmine-helpers": "^1.0.7",
    "@djforth/cookie_mgmt": "^1.0.1",
    "@djforth/ap_karma": "1.1.9",
    "@djforth/ap_utils": "^2.0.13",
    "angular": "^1.4.9",
    "angular-resource": "^1.4.9",
    "angular-mocks": "^1.4.9",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-uglify": "^1.5.1",
    "lodash": "~4.6.1"
  },
  "peerDependencies": {
    "@djforth/cookie_mgmt": "^1.0.1",
    "angular": "^1.4.9",
    "angular-resource": "^1.4.9",
    "lodash": "~4.6.1"
  }
}
